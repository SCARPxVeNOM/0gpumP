{
  "name": "0g-pump",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "vercel-build": "next build",
    "build:all": "npm run build",
    "lint": "next lint",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "dev:backend": "node server.js",
    "dev:indexer": "node server/indexer.js",
    "dev:kit": "npm run dev --prefix 0g-storage-ts-starter-kit",
    "dev:web": "next dev -p 3001",
    "dev:all": "concurrently -n kit,backend,web -c magenta,cyan,green \"npm run dev:kit\" \"npm run dev:backend\" \"npm run dev:web\"",
    "deploy:dex": "hardhat run scripts/deployRemainingContracts.js --network 0g-testnet",
    "deploy:curve": "hardhat run scripts/deployBondingCurve.js --network 0g-testnet",
    "deploy:migrator": "hardhat run scripts/deployMigrator.js --network 0g-testnet",
    "deploy:ogtoken": "hardhat run scripts/deployOGToken.js --network 0g-testnet",
    "enable:trading": "hardhat run scripts/enableTrading.js --network 0g-testnet",
    "setup:auto": "hardhat run scripts/autoTradingSetup.js --network 0g-testnet",
    "get:addresses": "hardhat run scripts/getTokenAddresses.js --network 0g-testnet",
    "setup:automated": "hardhat run scripts/automatedSetup.js --network 0g-testnet",
    "setup:from-storage": "hardhat run scripts/automatedSetupFromStorage.js --network 0g-testnet",
    "deploy:dex:core": "hardhat run scripts/deployCoreDEX.js --network 0g-testnet",
    "enable:existing": "hardhat run scripts/enableExistingTokens.js --network 0g-testnet",
    "find:tokens": "node scripts/findTokensFromStorage.js",
    "compile": "hardhat compile",
    "test:storage": "node test-0g-storage.js",
    "start:0g": "start-0g-integration.bat"
  },
  "dependencies": {
    "@0glabs/0g-ts-sdk": "^0.3.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@rainbow-me/rainbowkit": "^1.3.3",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "cors": "^2.8.5",
    "ethers": "^5.7.2",
    "express": "^5.1.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "multer": "^2.0.2",
    "next": "14.0.3",
    "pino-pretty": "^13.1.1",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "tailwind-merge": "^3.3.1",
    "wagmi": "^1.4.7"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/sqlite3": "^3.1.8",
    "autoprefixer": "^10.0.1",
    "chai": "^4.5.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.6.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "ethereum-waffle": "^4.0.10",
    "hardhat": "^2.26.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
